{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "hooks/*": ["hooks/*"],
      "components/*": ["components/*"],
      "theme/*": ["theme/*"],
      "navigation/*": ["navigation/*"],
      "screens/*": ["screens/*"],
      "constant/*": ["constants/*"]
    },
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
